<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.serch.cars_implements"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="26" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name="com.serch.cars_implements.activities.MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.serch.cars_implements.activities.cars_activity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.serch.cars_implements.activities.draw_activity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.serch.cars_implements.activities.person_activity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name="com.serch.cars_implements.activities.history_activity" />

        <service
            android:name="com.serch.cars_implements.service_sincrono"
            android:exported="false" />
    </application>

</manifest>